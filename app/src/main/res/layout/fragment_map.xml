<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments.MapFragment">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/google_map_fragment"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        map:cameraTargetLat="47.6222833"
        map:cameraTargetLng="-122.35217460000001"
        map:cameraZoom="17"
        tools:context=".MapsActivity" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabNearby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:backgroundTint="@color/colorPrimaryDark"
        app:elevation="17dp"
        app:fabSize="normal"
        app:layout_constraintBottom_toTopOf="@+id/mapFabAdd"
        app:layout_constraintEnd_toEndOf="parent"
        app:rippleColor="@color/colorPrimary"
        app:srcCompat="@android:drawable/ic_menu_view" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab1mi"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="16dp"
        android:clickable="true"
        android:scaleType="fitXY"
        app:backgroundTint="@color/colorPrimaryDark"
        app:fabSize="normal"
        app:layout_constraintEnd_toStartOf="@+id/fab10mi"
        app:layout_constraintTop_toBottomOf="@+id/fab5mi"
        app:rippleColor="@color/colorPrimary"
        app:srcCompat="@drawable/ic_map1" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab5mi"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"

        android:layout_marginEnd="20dp"
        android:clickable="true"
        android:src="@drawable/ic_map5"
        app:backgroundTint="@color/colorPrimaryDark"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="@+id/fab10mi"
        app:layout_constraintEnd_toStartOf="@+id/fab10mi"
        app:rippleColor="@color/colorPrimary" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab10mi"
        android:layout_width="118dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:scaleType="fitXY"

        android:src="@drawable/ic_map10"
        app:backgroundTint="@color/colorPrimaryDark"
        app:fabSize="normal"
        app:layout_constraintBottom_toTopOf="@+id/fabNearby"
        app:layout_constraintEnd_toEndOf="@+id/fabNearby"
        app:rippleColor="@color/colorPrimary" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/mapFabAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:backgroundTint="@color/colorPrimaryDark"
        app:elevation="17dp"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:rippleColor="@color/colorPrimary"
        app:srcCompat="@drawable/ic_add_white_24dp" />


</android.support.constraint.ConstraintLayout>
